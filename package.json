{
  "name": "portfolio-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && $npm_execpath sa-typegen && astro build",
    "preview": "astro build && wrangler pages dev",
    "astro": "astro",
    "deploy": "astro build && wrangler pages deploy",
    "cf-typegen": "wrangler types",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,astro}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,astro}\"",
    "sa-typegen": "$npm_execpath run sa-schema:extract && $npm_execpath run sa-typegen:generate && $npm_execpath run sa-typegen:lint",
    "sa-schema:extract": "sanity schema extract --enforce-required-fields --path ./sanity.schema.json",
    "sa-typegen:generate": "sanity typegen generate",
    "sa-typegen:lint": "eslint --fix ./sanity.types.ts"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.2.1",
    "@astrojs/react": "^4.2.0",
    "@astrojs/rss": "^4.0.11",
    "@fontsource-variable/dm-sans": "^5.1.1",
    "@fontsource-variable/hepta-slab": "^5.1.1",
    "@radix-ui/react-dialog": "^1.1.6",
    "@sanity/asset-utils": "^2.2.1",
    "@sanity/astro": "^3.2.5",
    "@sanity/client": "^6.28.0",
    "@sanity/code-input": "^5.1.2",
    "@sanity/image-url": "^1.1.0",
    "@sanity/ui": "^2.14.1",
    "@sanity/vision": "^3.76.3",
    "@sanity/visual-editing": "^2.13.5",
    "@shikijs/transformers": "^1.29.2",
    "@t3-oss/env-core": "^0.12.0",
    "@tabler/icons-react": "3.29.0",
    "@tailwindcss/vite": "^4.0.8",
    "astro": "^5.3.0",
    "astro-portabletext": "^0.11.1",
    "class-variance-authority": "^0.7.1",
    "dayjs": "^1.11.13",
    "groq": "^3.76.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.15.1",
    "sanity": "^3.76.3",
    "sanity-plugin-webhooks-trigger": "^0.3.1",
    "sonner": "^2.0.1",
    "styled-components": "^6.1.15",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.8",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250214.0",
    "@eslint/js": "^9.21.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.5.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.1.1",
    "wrangler": "^3.109.2"
  },
  "packageManager": "pnpm@9.9.0",
  "pnpm": {
    "patchedDependencies": {
      "@tabler/icons-react": "patches/@tabler__icons-react.patch"
    }
  }
}
