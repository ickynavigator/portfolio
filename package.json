{
  "name": "portfolio-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "prebuild": "$npm_execpath sa-typegen && $npm_execpath cf-typegen && astro check",
    "build": "astro build",
    "preview": "astro build && wrangler dev",
    "astro": "astro",
    "lint": "eslint --cache",
    "lint:fix": "eslint --fix --cache",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "typegen": "$npm_execpath sa-typegen && $npm_execpath cf-typegen",
    "cf-typegen": "$npm_execpath run cf-typegen:generate",
    "cf-typegen:generate": "wrangler types",
    "sa-dev": "sanity dev",
    "sa-deploy": "npx sanity schema deploy --manifest-dir ./public/studio/static",
    "sa-typegen": "$npm_execpath run sa-typegen:extract && $npm_execpath run sa-typegen:generate && $npm_execpath run sa-typegen:lint",
    "sa-typegen:extract": "sanity schema extract --enforce-required-fields --path ./sanity.schema.json",
    "sa-typegen:generate": "sanity typegen generate",
    "sa-typegen:lint": "eslint --fix ./sanity.types.ts"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/react": "^4.4.2",
    "@astrojs/rss": "^4.0.15",
    "@astrojs/sitemap": "^3.7.0",
    "@fontsource-variable/dm-sans": "^5.2.8",
    "@fontsource-variable/hepta-slab": "^5.2.8",
    "@imikailoby/lastfm-ts": "^2.0.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@sanity/asset-utils": "^2.3.0",
    "@sanity/astro": "^3.2.11",
    "@sanity/client": "^7.14.1",
    "@sanity/code-input": "^7.0.8",
    "@sanity/icons": "^3.7.4",
    "@sanity/image-url": "^2.0.3",
    "@sanity/ui": "^3.1.11",
    "@sanity/vision": "^5.8.1",
    "@sanity/visual-editing": "^5.2.1",
    "@shikijs/transformers": "^3.22.0",
    "@t3-oss/env-core": "^0.13.10",
    "@tabler/icons-react": "3.36.1",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.17.1",
    "astro-compress": "^2.3.9",
    "astro-portabletext": "^0.13.0",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "dayjs": "^1.11.19",
    "groq": "^5.8.1",
    "partysocket": "^1.1.11",
    "posthog-js": "^1.341.0",
    "posthog-node": "^5.24.10",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "recharts": "^2.15.4",
    "remeda": "^2.33.4",
    "sanity": "^5.8.1",
    "sanity-plugin-webhooks-trigger": "^0.7.0",
    "sonner": "^2.0.7",
    "styled-components": "^6.3.8",
    "tailwind-merge": "^3.4.0",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^24.10.11",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-unused-imports": "^4.3.0",
    "partykit": "0.0.115",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react-compiler-runtime": "19.1.0-rc.3",
    "react-is": "^19.2.4",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^6.4.1",
    "vite-tsconfig-paths": "^6.0.5",
    "wrangler": "^4.62.0"
  },
  "packageManager": "pnpm@10.28.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "core-js",
      "esbuild",
      "scrollmirror",
      "sharp",
      "workerd",
      "yarn"
    ]
  },
  "peerDependencies": {
    "react-compiler-runtime": "19.1.0-rc.3",
    "react-is": "^19.0.0"
  }
}