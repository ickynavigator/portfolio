{
  "name": "portfolio-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "prebuild": "$npm_execpath sa-typegen && $npm_execpath cf-typegen && astro check",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "astro": "astro",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typegen": "$npm_execpath sa-typegen && $npm_execpath cf-typegen",
    "cf-typegen": "$npm_execpath run cf-typegen:generate && $npm_execpath run cf-typegen:lint",
    "cf-typegen:generate": "wrangler types",
    "cf-typegen:lint": "eslint --fix ./worker-configuration.d.ts",
    "sa-typegen": "$npm_execpath run sa-typegen:extract && $npm_execpath run sa-typegen:generate && $npm_execpath run sa-typegen:lint",
    "sa-typegen:extract": "sanity schema extract --enforce-required-fields --path ./sanity.schema.json",
    "sa-typegen:generate": "sanity typegen generate",
    "sa-typegen:lint": "eslint --fix ./sanity.types.ts"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.6.0",
    "@astrojs/react": "^4.3.0",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.1",
    "@fontsource-variable/dm-sans": "^5.2.6",
    "@fontsource-variable/hepta-slab": "^5.2.6",
    "@radix-ui/react-dialog": "^1.1.14",
    "@sanity/asset-utils": "^2.2.1",
    "@sanity/astro": "^3.2.6",
    "@sanity/client": "^7.6.0",
    "@sanity/code-input": "^5.1.2",
    "@sanity/image-url": "^1.1.0",
    "@sanity/ui": "^2.16.2",
    "@sanity/vision": "^3.97.1",
    "@sanity/visual-editing": "^2.15.0",
    "@shikijs/transformers": "^3.7.0",
    "@t3-oss/env-core": "^0.13.8",
    "@tabler/icons-react": "3.34.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "astro": "^5.11.0",
    "astro-compress": "^2.3.8",
    "astro-portabletext": "^0.11.1",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "dayjs": "^1.11.13",
    "groq": "^3.97.1",
    "posthog-js": "^1.256.2",
    "posthog-node": "^5.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^2.15.4",
    "remeda": "^2.24.0",
    "sanity": "^3.97.1",
    "sanity-plugin-webhooks-trigger": "^0.4.1",
    "sonner": "^2.0.6",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.3.1",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5",
    "vaul": "^1.1.2",
    "zod": "^4.0.2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.9.0",
    "@eslint/js": "^9.30.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "lodash": "^4.17.21",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "react-compiler-runtime": "19.1.0-rc.2",
    "react-is": "^19.1.0",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^6.3.5",
    "wrangler": "^4.23.0"
  },
  "packageManager": "pnpm@10.12.4",
  "pnpm": {
    "patchedDependencies": {
      "@tabler/icons-react": "patches/@tabler__icons-react.patch"
    },
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "core-js",
      "esbuild",
      "scrollmirror",
      "sharp",
      "workerd",
      "yarn"
    ]
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "react-compiler-runtime": "19.1.0-rc.2",
    "react-is": "^19.0.0"
  }
}
