{
  "name": "portfolio-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "dev:sa": "sanity dev",
    "start": "astro dev",
    "prebuild": "$npm_execpath typegen && astro check",
    "build": "astro build",
    "deploy:sa": "sanity schema deploy --manifest-dir ./public/studio/static",
    "preview": "astro build && wrangler dev",
    "astro": "astro",
    "lint": "eslint --cache",
    "lint:fix": "eslint --fix --cache",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "typegen": "$npm_execpath typegen:sa && $npm_execpath typegen:cf",
    "typegen:cf": "wrangler types",
    "typegen:sa": "sanity schema extract && sanity typegen generate && eslint --fix --cache ./sanity.types.ts"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/react": "^4.4.2",
    "@astrojs/rss": "^4.0.15",
    "@astrojs/sitemap": "^3.7.0",
    "@fontsource-variable/dm-sans": "^5.2.8",
    "@fontsource-variable/hepta-slab": "^5.2.8",
    "@imikailoby/lastfm-ts": "^2.0.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@sanity/asset-utils": "^2.3.0",
    "@sanity/astro": "^3.2.11",
    "@sanity/client": "^7.15.0",
    "@sanity/code-input": "^7.0.8",
    "@sanity/icons": "^3.7.4",
    "@sanity/image-url": "^2.0.3",
    "@sanity/ui": "^3.1.11",
    "@sanity/vision": "^5.10.0",
    "@sanity/visual-editing": "^5.2.1",
    "@shikijs/transformers": "^3.22.0",
    "@t3-oss/env-core": "^0.13.10",
    "@tabler/icons-react": "3.36.1",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.2.0",
    "astro": "^5.17.3",
    "astro-compress": "^2.3.9",
    "astro-portabletext": "^0.13.0",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.1",
    "dayjs": "^1.11.19",
    "groq": "^5.10.0",
    "partysocket": "^1.1.13",
    "posthog-js": "^1.351.1",
    "posthog-node": "^5.24.17",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "recharts": "^2.15.4",
    "remeda": "^2.33.6",
    "sanity": "^5.10.0",
    "sanity-plugin-webhooks-trigger": "^0.7.0",
    "sonner": "^2.0.7",
    "styled-components": "^6.3.8",
    "tailwind-merge": "^3.5.0",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.2.0",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.6.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-unused-imports": "^4.4.1",
    "partykit": "0.0.115",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react-compiler-runtime": "19.1.0-rc.3",
    "react-is": "^19.2.4",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^6.4.1",
    "vite-tsconfig-paths": "^6.1.1",
    "wrangler": "^4.66.0"
  },
  "packageManager": "pnpm@10.28.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "core-js",
      "esbuild",
      "scrollmirror",
      "sharp",
      "workerd",
      "yarn"
    ]
  },
  "peerDependencies": {
    "react-compiler-runtime": "19.1.0-rc.3",
    "react-is": "^19.0.0"
  }
}